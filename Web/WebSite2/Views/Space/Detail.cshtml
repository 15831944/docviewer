@model WebSite2.Models.SpaceViewModel
@{
    ViewBag.Title = @Model.CurrentSpace.SpaceName;
}

<div class="row-fluid">

    <div class="col-md-3">

        @Html.Partial("Shared/_QuickMenu", @Model)

    </div>
    <div class="col-md-9 ">

        <ol class="breadcrumb">
            <li><a href="/Home/Space?Id=@Model.CurrentSpace.Id">@Model.CurrentSpace.SpaceName</a></li>
            <li class="active">文档</li>
        </ol>

        @Html.Partial("DocumentList", Model.Documents)
       
    </div>
</div>